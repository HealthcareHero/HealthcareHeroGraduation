{
  "files.exclude": {
    "**/node_modules": true,
    "**/.eslintrc.json": true,
    "**/package-lock.json": true,
    "**/.gitignore": true,
    "**/.firebaserc": true,
    "**/.firebase": true,
    "**/firebase.json": true,
    "**/README.md": true,
    "LICENSE": true,
    "**/serviceWorker.js": true,
    "**/setupTests.js": true,
    "client/public/favicon.ico": true,
    "client/public/*.png": true,
    "client/public/manifest.json": true,
    "client/public/browserconfig.xml": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  },
  "[css]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false
  }
}